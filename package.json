{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:pages": "vite build --mode pages",
    "deploy:pages": "yarn build:pages && rm -rf docs && mkdir docs && cp -r dist/* docs/ && cp docs/index.html docs/404.html && git add docs && git commit -m \"[deploy] git pages update\"",
    "preview": "vite",
    "tw:print": "tailwindcss --print-config -c ./tailwind.config.js",
    "export": "vite build --mode reactsnap && npx react-snap",
    "export:proof": "vite build && mkdir -p dist/testcases && cp dist/index.html dist/testcases/index.html"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "lucide-react": "^0.555.0",
    "prettier-eslint": "^16.4.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.63.0",
    "react-router-dom": "^7.9.1",
    "react-virtualized": "^9.22.6",
    "react-window": "1.8.11",
    "recharts": "^3.4.1",
    "recoil": "^0.7.7",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "tailwindcss": "3",
    "typescript": "^5.9.3",
    "vite": "^7.1.7"
  },
  "reactSnap": {
    "source": "dist",
    "crawl": false,
    "inlineCss": false,
    "port": 4567,
    "routes": [
      "/",
      "/testcases"
    ],
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  }
}
